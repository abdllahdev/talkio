{
  "name": "@talkio/deepgram",
  "version": "1.0.0-alpha.1",
  "description": "Deepgram STT and TTS providers for talkio",
  "keywords": [
    "deepgram",
    "speech-to-text",
    "stt",
    "talkio",
    "text-to-speech",
    "tts"
  ],
  "homepage": "https://github.com/abdllahdev/voice-ai/tree/main/packages/deepgram#readme",
  "bugs": {
    "url": "https://github.com/abdllahdev/voice-ai/issues"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdllahdev/voice-ai.git",
    "directory": "packages/deepgram"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build",
    "clean": "git clean -xdf node_modules dist .turbo .tsbuildinfo"
  },
  "devDependencies": {
    "@talkio/tsconfig": "workspace:*",
    "@types/node": "^22.10.2",
    "talkio": "workspace:*",
    "tsdown": "^0.19.0-beta.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "talkio": ">=1.0.0-alpha.1"
  },
  "engines": {
    "node": ">=20"
  }
}
