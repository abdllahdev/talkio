{
  "name": "voice-ai",
  "version": "0.1.0",
  "description": "Orchestration library for realtime voice AI agents",
  "homepage": "https://github.com/abdllahdev/voice-ai/tree/main/packages/voice-ai#readme",
  "bugs": {
    "url": "https://github.com/abdllahdev/voice-ai/issues"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdllahdev/voice-ai.git",
    "directory": "packages/voice-ai"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build",
    "clean": "rm -rf node_modules dist .tsbuildinfo coverage"
  },
  "dependencies": {
    "nanoid": "^5.1.6",
    "xstate": "^5.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@voice-ai/tsconfig": "workspace:*",
    "tsdown": "^0.19.0-beta.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20"
  }
}
