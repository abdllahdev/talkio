{
  "name": "talkio",
  "version": "1.0.0-alpha.1",
  "description": "Orchestration library for realtime voice AI agents",
  "homepage": "https://github.com/abdllahdev/voice-ai/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/abdllahdev/voice-ai/issues"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdllahdev/voice-ai.git",
    "directory": "packages/core"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build",
    "clean": "git clean -xdf node_modules dist .turbo .tsbuildinfo"
  },
  "dependencies": {
    "nanoid": "^5.1.6",
    "opus-decoder": "^0.7.11",
    "xstate": "^5.25.0"
  },
  "devDependencies": {
    "@talkio/tsconfig": "workspace:*",
    "@types/node": "^22.10.2",
    "tsdown": "^0.19.0-beta.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20"
  }
}
