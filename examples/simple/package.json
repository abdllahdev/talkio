{
  "name": "@talkio/simple-example",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf dist node_modules .turbo",
    "dev": "bun --hot src/server.ts",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/server.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.7",
    "@talkio/deepgram": "^1.0.0-alpha.1",
    "ai": "^6.0.22",
    "react": "^19",
    "react-dom": "^19",
    "talkio": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "@talkio/tsconfig": "workspace:*",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
